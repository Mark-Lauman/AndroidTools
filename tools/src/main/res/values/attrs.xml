<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SlidingTabLayout">
        <attr name="dividerColor" format="color" />
        <attr name="indicatorColor" format="color" />
    </declare-styleable>

    <declare-styleable name="MultiSelectPreference">
        <attr name="image" format="reference" />
        <attr name="name" format="string" />
        <attr name="nameColor" format="color" />
        <attr name="summary" format="string" />
        <attr name="summaryColor" format="color" />
        <attr name="summaryStatic" format="boolean" />
        <attr name="entries" format="reference" />
        <attr name="entryValues" format="reference" />
        <attr name="entryIcons" format="reference" />
        <attr name="entryIconValues" format="reference" />
        <attr name="key" format="string" />
        <attr name="inverted" format="boolean" />
    </declare-styleable>

    <declare-styleable name="CheckBoxPreference">
        <attr name="image" />
        <attr name="name" />
        <attr name="nameColor" />
        <attr name="summary" />
        <attr name="summaryColor" />
        <attr name="key" />
    </declare-styleable>

    <declare-styleable name="IntentPreference">
        <attr name="image" />
        <attr name="name" />
        <attr name="nameColor" />
        <attr name="summary" />
        <attr name="summaryColor" />
        <attr name="intent" format="string" />
    </declare-styleable>

    <declare-styleable name="PopupUndo">
        <attr name="text" format="string" />
        <attr name="hideDelay" format="integer" />
    </declare-styleable>
</resources>